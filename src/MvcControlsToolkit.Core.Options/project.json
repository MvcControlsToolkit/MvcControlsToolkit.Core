    "frameworks": {
        "net451": {
            "frameworkAssemblies": {

            }
        },
        "netstandard1.6": {
            "imports": [
                "dotnet5.6",
                "dnxcore50",
                "portable-net45+win8"
            ],
            "dependencies": {
                "Microsoft.CSharp": "4.3.0"
            }
        }
    },
    "scripts": {
        "postcompile": [ "dotnet pack --no-build --output /Nuget --configuration %compile:Configuration% %project:Directory%" ]
