{
    "description": "A Framework for handling options/preferences coming from several sources(user, browser capabilities, configuration, etc.) in Asp.net core",
    "title": "Mvc Controls Toolkit Options framework",
    "copyright": "Mvc Controls Toolkit Team",
    "version": "1.1.5.2",
    "authors": [ "Francesco Abbruzzese", "Mvc Controls Toolkit Team" ],
    "packOptions": {
        "projectUrl": "https://github.com/MvcControlsToolkit/MvcControlsToolkit.Core",
        "requireLicenseAcceptance": true,
        "releaseNotes": "Moved to Asp.net 1.1",
        "iconUrl": "https://avatars2.githubusercontent.com/u/15634526?v=3&s=200",
        "owners": [ "Francesco Abbruzzese", "Mvc Controls Toolkit Team" ],
        "licenseUrl": "https://github.com/MvcControlsToolkit/Home/blob/master/LICENSE.txt",
        "repository": {
            "type": "git",
            "url": "git://github.com/MvcControlsToolkit/MvcControlsToolkit.Core.git"
        },
        "tags": [
            "aspnetcore",
            "aspnetcoremvc"
        ]
    },
    "buildOptions": {
        "keyFile": "../../tools/Key.snk"
