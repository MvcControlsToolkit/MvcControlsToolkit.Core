@model ITestInterface
@using MvcControlsToolkit.Core.Views
@using WebTestCore.ViewModels


@{
    ViewData["Title"] = "Store special types test";
}

<h2>@ViewData["Title"].</h2>

<form asp-controller="Home" asp-action="JsonInterfaceTest" method="post" class="form-horizontal" role="form">
    <hr />
    <div asp-validation-summary="All" class="text-danger"></div>
    <store-model asp-for="@Model" encrypted="false" />

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button type="submit" class="btn btn-default">Submit</button>
        </div>
    </div>
</form>
