{
    "name": "jquery-validation-unobtrusive-extensions",
    "version": "1.0.0-rc3",
    "homepage": "https://github.com/MvcControlsToolkit/Unobtrusive.Extensions",
    "description": "Add-on to jQuery Unobtrusive Validation to enable globalization and dynamic content parsing",
    "main": [
        "jquery.validate.unobtrusive.dynamic.js",
        "jquery.validate.unobtrusive.globalize.js"
    ],
    "ignore": [
        "**/.*",
        "*.json",
        "*.md",
        "*.txt",
        "!LICENSE.txt",
        "gulpfile.js"
    ],
    "keywords": [
        "jquery",
        "asp.net",
        "mvc",
        "validation",
        "unobtrusive",
        "globalization"
    ],
    "authors": [
        "Mvc Controls Toolkit Team",
        "Francesco Abbruzzese"
    ],
    "license": "https://github.com/MvcControlsToolkit/Unobtrusive.Extensions/blob/master/LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "git://github.com/MvcControlsToolkit/Unobtrusive.Extensions.git"
    },
    "dependencies": {
        "jquery-validation-unobtrusive": ">=3.2.4",
        "jquery": ">=1.8", 
        "mvcct-enhancer": ">=1.0.0-rc3"
    }
}